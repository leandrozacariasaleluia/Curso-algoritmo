algoritmo "semnome"
// Função :
// Autor :
// Data : 20/05/2021
// Seção de Declarações 
var
     v: vetor[1..7] de inteiro
     i: inteiro
inicio
// Seção de Comandos 
para i <-1 ate 7 faca
     escreva (" DIGITE O " ,c, " o valor: ")
     leia(v[i])
fimpara
       para c <-1 ate 6 faca
            escreva ("{", v[c], " }")
       fimpara
       se (val[i]%2=0) entao
       
fimalgoritmo
